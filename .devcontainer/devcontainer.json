{
    "name": "LaTeXspace",
    "build": {
	"dockerfile": "./Dockerfile",
	"context": "."
    },
    "features": {
	"ghcr.io/devcontainers/features/common-utils:2": {
	    "installZsh": "true",
	    "username": "vscode",
	    "userUid": "1000",
	    "userGid": "1000",
	    "upgradePackages": "true"
	},
	"ghcr.io/devcontainers/features/git:1": {
            "version": "latest",
            "ppa": "false"
        }
    },
    "remoteUser": "vscode",
    "customizations": {
	"vscode": {
	    "settings": {
		"latex-workshop.latex.autoBuild.run": "never",
	    },
	    "extensions": [
		"James-Yu.latex-workshop"
	    ]
	}
    }
}
